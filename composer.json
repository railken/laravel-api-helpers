{
    "name": "railken/laravel-api-helpers",
    "keywords": ["api"],
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "railken/bag": "^1.2",
        "illuminate/database": "^5.4",
        "railken/search-query": "^2.1.7",
        "phpstan/phpstan": "^0.9.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.5",
        "friendsofphp/php-cs-fixer": "^2.8.1",
        "codeclimate/php-test-reporter": "^0.4.4"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Laravel\\ApiHelpers\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Railken\\Laravel\\ApiHelpers\\Tests\\": "tests/" }
    }
}
